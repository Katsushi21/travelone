###############
# Post
###############
type Post {
  id: ID!
  account_id: ID!
  title: String!
  body: String!
  img: String!
  createdAt: Time!
  updatedAt: Time!
  account: Account!
  marker: Marker
  like: [Like!]
  comment: [Comment!]
}

###############
# Input
###############
input PostInput {
  account_id: ID!
  title: String!
  body: String!
  img: String!
}
